{
    "name": "@theoparis/config",
    "version": "1.0.5",
    "description": "F*ck the config",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    },
    "scripts": {
        "clean": "rimraf dist",
        "tsc": "tsc --pretty",
        "build:prod": "pnpm run tsc -- -P tsconfig.prod.json",
        "build:test": "pnpm run tsc -- -P tsconfig.test.json",
        "pretest": "pnpm run clean && pnpm run build:test",
        "test": "ava test/**/*.{spec,e2e}.ts",
        "lint": "eslint lib/**/*.ts",
        "lint:fix": "pnpm run lint -- --fix",
        "prepublishOnly": "pnpm run lint:fix && pnpm run test && pnpm run build:prod"
    },
    "keywords": [
        "typescript",
        "config",
        "easy-to-use",
        "parser",
        "serializer",
        "json",
        "yml",
        "yaml",
        "ini",
        "toml",
        "json5",
        "hjson"
    ],
    "files": [
        "dist"
    ],
    "author": "Theo Paris <theoparisdesigns@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git@github.com:creepinson/ftconfig.git"
    },
    "bugs": {
        "url": "https://github.com/creepinson/ftconfig/issues"
    },
    "homepage": "https://github.com/creepinson/ftconfig#readme",
    "license": "MIT",
    "yVersion": "2.1.25",
    "devDependencies": {
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@commitlint/lint": "12.1.4",
        "@iarna/toml": "2.2.5",
        "@types/hjson": "2.4.2",
        "@types/ini": "1.3.30",
        "@types/js-yaml": "4.0.1",
        "@types/node": "15.12.4",
        "@typescript-eslint/eslint-plugin": "4.27.0",
        "@typescript-eslint/parser": "4.27.0",
        "ava": "3.15.0",
        "eslint": "7.29.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "3.4.0",
        "find-up": "5.0.0",
        "husky": "6.0.0",
        "lint-staged": "11.0.0",
        "prettier": "2.2.1",
        "rimraf": "3.0.2",
        "ts-node": "10.1.0",
        "typescript": "4.3.5"
    },
    "dependencies": {
        "@iarna/toml": "^2.2.5",
        "hjson": "^3.2.2",
        "ini": "^2.0.0",
        "js-yaml": "^4.1.0",
        "json5": "^2.2.0",
        "zod": "^3.2.0"
    },
    "peerDependencies": {
        "@iarna/toml": "^2.2.5",
        "hjson": "^3.2.2",
        "ini": "^2.0.0",
        "js-yaml": "^4.1.0",
        "json5": "^2.2.0",
        "zod": "^3.2.0"
    }
}
